{
  "name": "salon-fenix",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.4",
  "sideEffects": false,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all",
    "last 10 Edge versions",
    "last 10 Chrome versions",
    "last 10 Firefox versions"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "dev": "next dev",
    "start": "NODE_ENV=production next start",
    "build": "next build",
    "preview": "next start -p 4000",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "prettier:show": "prettier --list-different \"./**/*\"",
    "prettier:write": "prettier \"./**/*\" --write",
    "clean": "rm -r -f dist .next package-lock.json yarn.lock pnpm-lock.yaml node_modules && pnpm install"
  },
  "dependencies": {
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/postcss": "4.1.16",
    "@next/env": "^16.0.0",
    "@next/third-parties": "^16.0.0",
    "@google-recaptcha/react": "^2.3.2",
    "babel-loader": "^10.0.0",
    "@formatjs/intl": "3.1.8",
    "negotiator": "1.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "@formatjs/intl-localematcher": "0.6.2",
    "core-js": "^3.46.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "intl": "^1.2.5",
    "postcss": "8.5.6",
    "next": "16.0.1",
    "leaflet": "^1.9.4",
    "mailersend": "^2.6.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intl": "7.1.14",
    "tailwindcss": "4.1.16",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@types/negotiator": "0.6.4",
    "@next/eslint-plugin-next": "16.0.0",
    "@types/node": "24.9.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@types/leaflet": "^1.9.21",
    "@types/madge": "5.0.3",
    "@types/react": "19.2.2",
    "@types/eslint": "9.6.1",
    "@types/intl": "1.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.39.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-formatjs": "5.4.2",
    "eslint-plugin-i18next": "6.1.3",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-typegen": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "madge": "8.0.0",
    "rimraf": "6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "typescript": "5.9.3"
  }
